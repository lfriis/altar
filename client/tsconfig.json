{
	"compilerOptions": {
		"target": "es6",
		"lib": [
			"ES5",
			"ES2015",
			"ES2016",
			"ES6",
			"DOM",
			"DOM.Iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noFallthroughCasesInSwitch": true,
		"module": "esnext",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictNullChecks": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true,
		"jsx": "react"
	},
	"include": ["src"]
}
